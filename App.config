<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- Name of the printer -->
    <add key="printer" value="Evolis Zenius"/>
    
    <!-- Print in landscape? -->
    <add key="landscape" value="True" />

    <!-- Ratios to multiply image dimensions by, e.g. 2000px * 0.5 => 1000px -->
    <add key="width" value="0.678"/>
    <add key="height" value="0.679"/>

    <!-- File extensions to watch out for -->
    <add key="fileExtensions" value="jpg,jpeg,png"/>

    <!-- Once an image has been received and sent to the printer, this represents the amount of seconds this app will temporarily ignore further incoming images -->
    <add key="lockedDuration" value="10.0"/>
  </appSettings>
</configuration>